{
  "version": 2,
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "VITE_API_URL": "@vite_api_url",
    "VITE_CLERK_PUBLISHABLE_KEY": "@vite_clerk_publishable_key",
    "VITE_AGORA_APP_ID": "@vite_agora_app_id"
  }
}